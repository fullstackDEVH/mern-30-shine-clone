{
    "extends" : ["react-app", "prettier"],
    "plugins" : ["react", "prettier"],
    "rules" : {
        "prettier/prettier" : [
            "warn", {
                "arrowParens": "always",
                "semi": true, 
                "trailingComma": "none",
                "tabWidth": 4,
                "endOfLine": "auto",
                "printWidth": 120   
            }
        ]
    }
}